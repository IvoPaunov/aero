{
	"env": {
		"node": true
	},
	"ecmaFeatures": {
		"arrowFunctions": false,
		"blockBindings": true,
		"forOf": true,
		"generators": true
	},
	"rules": {
		"strict": 0,
		"eqeqeq": 2,
		"curly": 0,
		"quotes": [2, "double"],
		"no-trailing-spaces": 0,
		"no-extend-native": 0,
		"eol-last": 0
	}
}