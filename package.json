{
	"name": "aero",
	"version": "0.3.1",
	"description": "High-performance website framework based on io.js, Jade and Stylus.",
	"author": "Eduard Urbach <e.urbach@gmail.com> (http://blitzprog.org)",
	"license": "GPL-3.0",
	"homepage": "https://github.com/freezingwind/aero",
	"main": "src/index.js",
	"dependencies": {
		"ws": "*",
		"etag": "*",
		"jade": "*",
		"http2": "*",
		"stylus": "*",
		"bluebird": "*",
		"mmmagic": "*",
		"uglify-js": "*",
		"node-watch": "*",
		"object-assign": "*"
	},
	"devDependencies": {
		"mocha": "*",
		"istanbul": "*",
		"coveralls": "*",
		"supertest": "*"
